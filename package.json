{
  "name": "akeneo-node",
  "version": "0.0.5-beta.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "ajmnz <arnau@ajb.cat>",
  "license": "MIT",
  "description": "Type-safe Node.js client for the Akeneo PIM REST API",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ajmnz/akeneo-node"
  },
  "keywords": [
    "akeneo",
    "akeneo api client",
    "akeneo node.js",
    "pim"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf -rf dist tsconfig.tsbuildinfo",
    "prerelease": "yarn prerelease:npm && yarn prerelease:github",
    "prerelease:npm": "release-it --preReleaseId=beta --npm.tag=beta",
    "prerelease:github": "release-it --preReleaseId=beta --no-npm --no-git.requireCleanWorkingDir",
    "release": "yarn release:npm && yarn release:github",
    "release:npm": "release-it",
    "release:github": "yarn release --no-git.requireCleanWorkingDir --no-npm",
    "lint": "eslint --ext=ts src",
    "format": "prettier --write \"./src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "0.27.2",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.26",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "dotenv": "16.0.1",
    "eslint": "8.14.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "28.1.0",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "release-it": "15.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.3",
    "typescript": "4.6.3"
  }
}
